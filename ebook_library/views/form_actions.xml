<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="ebook_library.model_ebook_library_lead" model="ir.model">
      <field name="website_form_key">download_ebook</field>
      <field name="website_form_access">True</field>
      <field name="website_form_label">Download eBook</field>
    </record>

    <template id="ebook_library.assets_editor" name="eBook Library Assets Editor" inherit_id="website.assets_editor">
      <xpath expr="." position="inside">
        <script type="text/javascript" src="/ebook_library/static/src/js/ebook_library_editor.js"/>
      </xpath>
    </template>
  </data>
</odoo>
