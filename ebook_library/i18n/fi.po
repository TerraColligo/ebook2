#. module: ebook_library
#: model:mail.template,body_html:ebook_library.ebook_download_email
msgid ""
"\n"
"    \n"
"      <p>Hello ${object.name}!</p>\n"
"      <p>You can download ${object.ebook.name} here:</p>\n"
"        % for file in object.ebook.downloadable_files:\n"
"        <p><a target=\"_blank\" rel=\"noopener\" href=\"${ctx.download_url}&file_id=${file.id}\">Download ${file.filename}</a></p>\n"
"        % endfor\n"
"    \n"
"    "
msgstr ""
"\n"
"    \n"
"      <p>Hei ${object.name}!</p>\n"
"      <p>Kiitos mielenkiinnostasi eKirjaa ${object.ebook.name} kohtaan. Voit ladata sähköisen kirjan täältä:</p>\n"
"        % for file in object.ebook.downloadable_files:\n"
"        <p><a target=\"_blank\" rel=\"noopener\" href=\"${ctx.download_url}&file_id=${file.id}\">Lataa ${file.filename}</a></p>\n"
"        % endfor\n"
"    \n"
"    "

